
 <%= form_with model: @target, url: { action: :create }, local: true do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title, autofocus: true,  placeholder: "目標名" %>
      <%= f.label :price %>
      <%= f.text_field :price, autofocus: true,  placeholder: "金額" %>

      <%= f.label :start_time %>
      <%= f.datetime_select :start_time %>


      <%= f.submit "作成" %>
 <% end %>
