<h1>投稿一覧</h1>
<div class="container">
  <div class="row">
    <table class="table">
	    <thead>
        <tr>
          <th>user</th>
          <th>title</th>
          <th>price</th>
        </tr>
      </thead>
      <% @targets.each do |target| %>
        <tbody>
  	      <tr>
            <td><%= link_to user_path(target.user.id) do %>
                  <%= target.user.name %>
                <% end %></td>
            <td><%= link_to target.title,target_path(target) %></td>
            <td><%= target.price %></td>
          </tr>
        </tbody>
      <% end %>
    </div>
  </div>
</div>